{
    "name": "@kit/shared",
    "description": "Logic shared between the apps. Used in the applications, no use packages because it aims to have lots of workspace packages dependencies.",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "clean": "git clean -xdf .turbo node_modules",
        "typecheck": "tsc --noEmit"
    },
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "react-native": "./src/index.ts",
    "files": [
        "src"
    ],
    "exports": {
        "./config/*": "./src/config/*.ts",
        "./lib/*": "./src/lib/*.ts",
        "./types/*": "./src/types/*.ts",
        "./server/*": "./src/server/*.ts",
        "./trpc-client-provider": "./src/trpc-client-provider.tsx",
        "./envs": "./envs.ts"
    },
    "devDependencies": {
        "@kit/tsconfig": "workspace:*",
        "@types/react": "~19.1.10"
    },
    "dependencies": {
        "@kit/ai": "workspace:*",
        "@kit/drizzle": "workspace:*",
        "@kit/billing": "workspace:*",
        "@kit/content-type": "workspace:*",
        "@kit/organization": "workspace:*",
        "@kit/notification": "workspace:*",
        "@kit/keybindings": "workspace:*",
        "@kit/supabase-client": "workspace:*",
        "@kit/settings": "workspace:*",
        "@kit/auth": "workspace:*",
        "@kit/email-templates": "workspace:*",
        "@kit/emailer": "workspace:*",
        "@kit/utils": "workspace:*",
        "@creatorem/next-trpc": "^1.0.16",
        "@t3-oss/env-nextjs": "0.13.8",
        "zod": "^3.25.76"
    },
    "peerDependencies": {
        "@kit/db": "workspace:*",
        "@kit/supabase-server": "workspace:*"
    },
    "peerDependenciesMeta": {
        "@kit/supabase-server": {
            "optional": true
        },
        "@kit/db": {
            "optional": true
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/*"
            ]
        }
    }
}
