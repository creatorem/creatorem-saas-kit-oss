    "private": true,
    "type": "module",
    "scripts": {
        "analyze": "ANALYZE_BUNDLE=true pnpm run build",
        "build": "pnpm with-env next build",
        "build:test": "NODE_ENV=test pnpm with-env:test next build",
        "clean": "git clean -xdf .cache .next .turbo build dist node_modules",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "dev": "pnpm with-env next dev --port 3000 --turbo",
        "start": "pnpm with-env next start --port 3000",
        "start:test": "NODE_ENV=test pnpm with-env:test next start",
        "lint": "pnpm exec biome check",
        "lint:fix": "pnpm exec biome check --fix",
        "typecheck": "tsc --noEmit",
        "with-env": "dotenv -e ./.env.local --",
        "with-env:test": "dotenv -e ./.env.test --",
        "detect-untranslated": "tsx ../../tooling/scripts/src/detect-untranslated-text.ts"
    },
