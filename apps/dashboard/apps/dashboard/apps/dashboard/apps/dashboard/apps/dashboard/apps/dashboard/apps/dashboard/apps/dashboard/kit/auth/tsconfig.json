{
    "extends": "@kit/tsconfig/base.json",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": ["./src/*"],
            "~/config": ["./src/config.ts"]
        },
        "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
    },
    "include": ["i18next.d.ts", "*.ts", "src", "@types", "./uniwind-types.d.ts"],
    "exclude": ["node_modules"]
}
