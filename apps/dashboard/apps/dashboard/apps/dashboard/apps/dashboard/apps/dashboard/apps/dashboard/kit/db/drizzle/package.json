{
    "name": "@kit/drizzle",
    "description": "Drizzle ORM for Supabase, with RLS support.",
    "private": true,
    "version": "0.0.0",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "react-native": "./src/index.ts",
    "files": [
        "src"
    ],
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./envs": {
            "types": "./envs.ts",
            "import": "./envs.ts"
        }
    },
    "scripts": {
        "clean": "git clean -xdf .turbo node_modules",
        "drizzle": "drizzle-kit",
        "drizzle:pull": "drizzle-kit pull --config drizzle.config.ts && ./scripts/fix-schema.sh && ./scripts/generate-table-types.sh",
        "lint": "pnpm exec biome check",
        "lint:fix": "pnpm exec biome check --fix",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@t3-oss/env-nextjs": "0.13.8",
        "drizzle-orm": "^0.43.1",
        "pg": "^8.16.3"
    },
    "devDependencies": {
        "@kit/tsconfig": "workspace:*",
        "@supabase/ssr": "^0.5.2",
        "@supabase/supabase-js": "^2.55.0",
        "@tanstack/react-query": "5.90.12",
        "@types/pg": "^8.15.5",
        "@types/react": "~19.1.10",
        "drizzle-kit": "^0.31.4",
        "next": "16.0.8",
        "react": "19.1.0",
        "server-only": "^0.0.1",
        "zod": "^3.25.76"
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/*"
            ]
        }
    }
}
